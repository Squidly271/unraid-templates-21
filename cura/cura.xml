  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/mikeah/cura-novnc</Support>
  <Project>https://ultimaker.com/software/ultimaker-cura</Project>
